<data>

<!--campos creados por herencia desde for_dependencias-->
    <record model="ir.ui.view" id="view_for_dependencias_form_inherit">
        <field name="name">for.dependencias.form.inherit</field>
        <field name="model">for.dependencias</field>
        <field name="type">form</field>
        <field name="inherit_id" ref="formacion_2015_base.view_for_dependencias_form"/>
        <field name="arch" type="xml">
            <field name="descripcion" position="after">
                <separator string="Maestros" colspan="4"/>
                <field name="maestro_ids" select="1" nolabel="1" colspan="4"/>
            </field>
        </field>
    </record>

<!--fin de campos creados por herencia desde for_dependencias-->


<!--campos creados por herencia desde formacion_pis_nomina_maestros-->
    <record model="ir.ui.view" id="view_for_pis_mae_asistencias_form_inherit">
        <field name="name">for.pis.mae_asistencias.form.inherit</field>
        <field name="model">for.pis.mae_asistencias</field>
        <field name="type">form</field>
        <field name="inherit_id" ref="formacion_2015_nomina_maestros.view_for_pis_mae_asistencias_form"/>
        <field name="arch" type="xml">
            <field name="numero_id" position="after">
                <field name="maestro_id" select="1" required="True" domain="[('pis_participado.numero_id','=',numero_id)]"/>
            </field>
        </field>
    </record>

    <record model="ir.ui.view" id="view_for_pis_mae_asistencias_tree_inherit">
        <field name="name">for.pis.mae_asistencias.tree.inherit</field>
        <field name="model">for.pis.mae_asistencias</field>
        <field name="type">tree</field>
        <field name="inherit_id" ref="formacion_2015_nomina_maestros.view_for_pis_mae_asistencias_tree"/>
        <field name="arch" type="xml">
            <field name="horas_lunes" position="before">
                <field name="maestro_id"/>
            </field>
        </field>
    </record>
<!--fin de campos creados por herencia desde formacion_pis_nomina_maestros-->

<!-- Acá inicia la clase heredada de formacion_pis_base-->
     <record model="ir.ui.view" id="view_for_pis_registro_inicial_form_inherit">
        <field name="name">for.pis.maestro.registro_inicial.form.inherit</field>
        <field name="model">for.pis.registro_inicial</field>
